{
  "createfdTicket": {
    "schema": {
      "method": "POST",
      "host": "<%= iparam.freshdesk_subdomain %>.freshdesk.com",
      "path": "/api/v2/tickets",
      "headers": {
        "Authorization": "Basic <%= encode(iparam.freshdesk_api_key) %>",
        "Content-Type": "application/json"
      }
    }
  },
  "createfsTicket": {
    "schema": {
      "method": "POST",
      "host": "<%= iparam.freshservice_subdomain %>.freshservice.com",
      "path": "/api/v2/tickets",
      "headers": {
        "Authorization": "Basic <%= encode(iparam.freshservice_api_key) %>",
        "Content-Type": "application/json"
      }
    }
  }
}
